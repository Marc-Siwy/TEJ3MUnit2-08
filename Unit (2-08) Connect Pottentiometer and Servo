//Output potentiometer to Serial Monitor in Arduino

#include <Servo.h>;

Servo realservo;

 int sensorValue;

void setup()  {
  Serial.begin(9600);
  realservo.attach(8);
  realservo.write(0);
  delay(1000);
  

}

 void loop() {
  int sensorValue = analogRead(A0);
  int angle = sensorValue / 5.6;
  Serial.println(sensorValue);
  delay(1);  
  realservo.write(angle);
 
}
