//Output potentiometer to Serial Monitor in Arduino

#include <Servo.h>;

Servo realservo;

 int sensorValue;

void setup()  {
  Serial.begin(9600);
  realservo.attach(8);
  realservo.write(0);
  delay(1000);
  int sensorValue/(5.555) = ServoAngle
  //set answer of division to angle of servo
  

}

 void loop() {
  int sensorValue = analogRead(A0);
  Serial.println(sensorValue);
  delay(1);  
  realservo.write(ServoAngle);
 
}
